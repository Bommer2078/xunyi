<!--pages/classic/classic.wxml-->
<view class="header">
    <epsoide-cmp class="epsoide" classicIndex="{{classicIndex}}" />
    <like-cmp
            count="{{count}}"
            isLike="{{isLike}}"
            class="like"
            bind:like="like"
    />
</view>
<movie-cmp classicImg="{{classicImg}}" classicContent="{{classicContent}}" />
<!--<music-cmp classicImg="{{classicImg}}" classicContent="{{classicContent}}" />-->
<navi-cmp
        classicTitle="{{classicTitle}}"
        currIndex="{{currIndex}}"
        lastIndex="{{lastIndex}}"
        class="navi"/>